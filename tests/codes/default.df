class B
{
	z:int
}



class A:B
{
	a:int;
	b:int;
	func foo(int c):int
	{
		return a+c+z
	}

	func bar():int
	{
		return 666;
	}


}

extension A
{
	func baz():int
	{
		return 666;
	}
	func foo(int a,int b):int
	{
		return a+b;
	}
}

extension A => func smt():int
{
	return a+666;
}


var a =A()
a.a=233
a.b=666
a.z=10000

printf("Hello,World\n")
printf("a's values: %d,%d,%d \n",a.a,a.b,a.z)
var b = 600
printf("overload a.foo(b):      %d\n",a.foo(b))
printf("overload a.foo(b,200):  %d\n",a.foo(b,200))

b = 2333
var c =A().bar()
printf("A().bar(): %d\n",A().bar())
printf("%d\n",b)
printf("%d\n",c)


